{
  "require": {
    "craftcms/cms": "3.0.26.1",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/contact-form": "^2.2",
    "fatfish/navigation": "v1.0.7",
    "craftcms/ckeditor": "1.0.0-beta.2",
    "rias/craft-contact-form-extensions": "1.0.11",
    "verbb/navigation": "1.0.12"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
